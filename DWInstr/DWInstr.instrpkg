<?xml version="1.0" encoding="UTF-8" ?>
<package>
  <id>com.bamtech.DWInstr</id>
  <title>DWInstr</title>
  <owner>
    <name>Donny Wals</name>
  </owner>

  <os-signpost-interval-schema>
    <id>click-track</id>
    <title>Click tracker</title>

    <subsystem>"com.dw"</subsystem>
    <category>"Behavior"</category>
    <name>"Click"</name>

    <start-pattern>
      <message>"Clicked at: " ?row</message>
    </start-pattern>

    <end-pattern>
        <message>"Clicked at: " ?row</message>
    </end-pattern>

    <column>
      <mnemonic>click</mnemonic>
      <title>Click</title>
      <type>string</type>
      <expression>?row</expression>
    </column>
  </os-signpost-interval-schema>

  <instrument>
    <id>buttontracker</id>
    <title>Button Tracker</title>
    <category>Behavior</category>
    <purpose>Instrument tracking cell clicks</purpose>
    <icon>Generic</icon>

    <create-table>
      <id>click-table</id>
      <schema-ref>click-track</schema-ref>
    </create-table>

    <graph>
      <title>Clicks</title>
      <lane>
        <title>Clicks</title>
        <table-ref>click-table</table-ref>

        <plot>
          <value-from>click</value-from>
        </plot>
      </lane>
    </graph>

    <list>
      <title>Clicks</title>
      <table-ref>click-table</table-ref>
      <column>click</column>
    </list>
  </instrument>
</package>
